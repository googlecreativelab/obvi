{
  "description": "One Button for Voice Input",
  "keywords": [
    "voice",
    "speech-recognition",
    "speech recognition",
    "asr",
    "polymer",
    "button"
  ],
  "repository": "https://github.com/googlecreativelab/obvi",
  "homepage": "https://googlecreativelab.github.io/obvi/",
  "name": "obvi",
  "version": "3.0.0",
  "main": "index.js",
